					<p><%= t "contact.opening_hours.title" %></p>
					<%= t "contact.opening_hours.mon_fri" %>: 12<sup>00</sup>-22<sup>00</sup> <span>(<%= t "contact.opening_hours.order_up_to" %> 21:30)</span>
					<br><%= t "contact.opening_hours.sat" %>: 13<sup>00</sup>-22<sup>00</sup> <span>(<%= t "contact.opening_hours.order_up_to" %> 21:30)</span>
					<br><%= t "contact.opening_hours.sun" %>: 13<sup>00</sup>-21<sup>00</sup> <span>(<%= t "contact.opening_hours.order_up_to" %> 20:30)</span>
					<p><%= t "address.title" %></p>
					<%= t "address.description" %>
					<br>Tel.: <a href="tel:888222750">888 222 750</a>
					<br><a href="mailto:barnburger@barnburger.pl">barnburger@barnburger.pl</a>
					<br>
					<div class="fb-like" data-href="http://www.facebook.com/barnburger" data-send="true" data-width="400" data-show-faces="true" data-font="arial" data-colorscheme="dark"></div>
				</div>
				<div class="kontakt-mapka">
					<p><%= t "map.title" %></p>
				    <div id="mapa-box" style="float:left;width:100%;height:50vh;"></div>
					<script src="https://maps.googleapis.com/maps/api/js?v=3.exp&sensor=false"></script>
					<script type="text/javascript"> 
					initialize();
					  function initialize() {
					  var myLatlng = new google.maps.LatLng(52.232710, 21.011438);
					  var mapOptions = {
					    zoom: 16,
					    center: myLatlng,
					    scrollwheel:true,
					    styles: [{"stylers":[{"hue":"#ff8800"},{"gamma":0.4}]}],
					    mapTypeId: google.maps.MapTypeId.ROADMAP
					  }
					 
					  var map = new google.maps.Map(document.getElementById('mapa-box'), mapOptions);
					 
					  var infowindow = new google.maps.InfoWindow({
					      content: '<div style="width:170px; height:70px">Barn Burger<br>ul. Złota 9<br>Warszawa<br>tel.: 888 222 750<br><a href="http://facebook.com/BarnBurger" target="_blank">facebook.com/BarnBurger</a></div>'
					  });
					  var image = 'http://barnburger.pl/img/mapslogo.png';
					  var marker = new google.maps.Marker({
					      position: myLatlng,
					      map: map,
					      icon: image,
					      title: 'Barn Burger'
					  });
					  google.maps.event.addListener(marker, 'click', function() {
					    infowindow.open(map,marker);
					  });
					}
					 
					google.maps.event.addDomListener(window, 'load', initialize);
					</script>
				</div>
