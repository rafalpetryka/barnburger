		<script>
			function flash_message(text){
				$.notify.addStyle('dark', {
					html: "<div><span data-notify-text/></div>",
					classes: {
					base: {}
				}
			});
			text=text.replace("&amp;", "&");
			$.notify(text, {autoHide: true, className: 'info', globalPosition: 'bottom left', autoHideDelay: 10000, showAnimation: 'slideDown', showDuration: 1000, style: 'dark'});
			}
			function add_to_favourite_burger(name){
				if (document.cookie.indexOf(name.replace(",", ""))!=-1){
					document.cookie = "fav_"+name.replace(",", "")+"="+"-1";
				}else{
					document.cookie = "fav_"+name.replace(",", "")+"="+"1";
					$(name.replace(",", "")).slideDown(function() {
					    setTimeout(function() {
					        $(name.replace(",", "")).slideUp();
					    }, 5000);
					});
				}
			}
		</script>
